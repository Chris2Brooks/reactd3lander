{
    "name": "gatsby-starter-default",
    "private": true,
    "description": "A simple starter to get up and developing quickly with Gatsby",
    "version": "0.1.0",
    "author": "Kyle Mathews <mathews.kyle@gmail.com>",
    "dependencies": {
        "@raae/gatsby-remark-oembed": "^0.0.7",
        "babel-plugin-styled-components": "^1.10.1",
        "bigmac-index-price-calculator": "^1.2.0",
        "dotenv": "^8.0.0",
        "gatsby": "^2.13.0",
        "gatsby-image": "^2.2.0",
        "gatsby-plugin-facebook-pixel": "^1.0.3",
        "gatsby-plugin-google-analytics": "^2.1.0",
        "gatsby-plugin-manifest": "^2.2.0",
        "gatsby-plugin-offline": "^2.2.0",
        "gatsby-plugin-react-helmet": "^3.1.0",
        "gatsby-plugin-sharp": "^2.2.0",
        "gatsby-plugin-styled-components": "^3.1.0",
        "gatsby-plugin-typography": "^2.3.2",
        "gatsby-remark-a11y-emoji": "^1.0.0",
        "gatsby-remark-copy-linked-files": "^2.1.0",
        "gatsby-remark-images": "^3.1.0",
        "gatsby-remark-prismjs": "^3.3.0",
        "gatsby-remark-responsive-iframe": "^2.2.0",
        "gatsby-remark-smartypants": "^2.1.0",
        "gatsby-source-filesystem": "^2.1.0",
        "gatsby-transformer-remark": "^2.6.0",
        "gatsby-transformer-sharp": "^2.2.0",
        "image-size": "^0.7.4",
        "lodash": "^4.17.11",
        "prismjs": "^1.16.0",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-collapse": "^5.0.0",
        "react-device-detect": "^1.6.5",
        "react-dom": "^16.8.6",
        "react-helmet": "^5.2.1",
        "react-lazyload-fadein": "^1.1.0",
        "react-tweet-embed": "^1.2.2",
        "react-typography": "^0.16.19",
        "react-youtube": "^7.9.0",
        "styled-components": "^4.3.2",
        "typography": "^0.16.19",
        "typography-theme-fairy-gates": "^0.16.19"
    },
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "bootstrap": "cd plugins/gatsby-remark-giphy && yarn install && cd ../gatsby-remark-youtube && yarn install && cd ../gatsby-remark-convertkit-form && yarn install && cd ../../",
        "build": "yarn bootstrap && gatsby build",
        "develop": "gatsby develop",
        "format": "prettier --write ./**/*.{js,ts,tsx,json,md,mdx,html,css}",
        "now-build": "yarn build",
        "serve": "gatsby serve",
        "start": "rm -rf ./.cache ./public && yarn develop",
        "watch": "nodemon",
        "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gatsbyjs/gatsby-starter-default"
    },
    "bugs": {
        "url": "https://github.com/gatsbyjs/gatsby/issues"
    },
    "prettier": {
        "printWidth": 79,
        "proseWrap": "always",
        "endOfLine": "lf",
        "semi": false,
        "singleQuote": true,
        "tabWidth": 2,
        "trailingComma": "es5"
    },
    "nodemonConfig": {
        "ignore": [
            ".DS_Store",
            ".cache",
            ".git",
            "node_modules",
            "public",
            "yarn-error.log",
            "yarn.lock",
            "*flycheck*"
        ],
        "watch": [
            "content",
            "gatsby-browser.js",
            "gatsby-config.js",
            "gatsby-node.js",
            "package.json",
            "src",
            "tsconfig.json"
        ],
        "ext": "js,ts,tsx,md",
        "verbose": true,
        "exec": "yarn build && yarn serve"
    },
    "devDependencies": {
        "nodemon": "^1.19.1",
        "now": "^16.6.3"
    }
}
